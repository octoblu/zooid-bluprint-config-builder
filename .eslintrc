{
  "extends": "airbnb",
  "rules": {
    "react/jsx-no-bind": 0,
    "react/prefer-stateless-function": 0,
    "arrow-body-style": 0,
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "no-multi-spaces": 0,
    "no-unused-expressions": 0,
    "semi": [2, "never"],
    "padded-blocks": 0,
    "object-curly-spacing": 0,
    "consistent-return": 0,
    "max-len": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "mocha": true
  }
}
