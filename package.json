{
  "name": "zooid-ui-bluprint-config-builder",
  "version": "0.0.1",
  "description": "BluprintConfigBuilder",
  "main": "dist/bundle.js",
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9001",
    "build": "webpack",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "test": "nyc --require babel-core/register --require babel-polyfill mocha",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octoblu/zooid-ui-bluprint-config-builder.git"
  },
  "keywords": [
    "zooid",
    "zooid-ui",
    "react",
    "reactjs",
    "octoblu"
  ],
  "author": "Octoblu Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octoblu/zooid-ui-bluprint-config-builder/issues"
  },
  "homepage": "https://github.com/octoblu/zooid-ui-bluprint-config-builder#readme",
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "max-len": 0,
      "semi": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./internals/webpack/webpack.test.babel.js"
        }
      }
    }
  },
  "nyc": {
    "exclude": [
      "src/**/*.spec.js",
      "dist"
    ]
  },
  "devDependencies": {
    "@kadira/storybook": "^1.28.1",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.2",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "ignore-styles": "^2.0.0",
    "jsdom": "^9.1.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "mocha-multi": "^0.9.0",
    "mocha-osx-reporter": "^0.1.2",
    "null-loader": "^0.1.1",
    "nyc": "^6.4.4",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "json-schema-faker": "^0.3.3",
    "json-schema-instantiator": "^0.4.0",
    "lodash": "^4.13.1",
    "react": "^15.0.1",
    "react-jsonschema-form": "^0.33.1",
    "zooid-ui": "^6.0.0"
  }
}
