{
  "name": "zooid-ui-bluprint-config-builder",
  "version": "8.0.4",
  "description": "BluprintConfigBuilder",
  "main": "dist/bundle.js",
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9001",
    "build": "webpack",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-",
    "coverage": "nyc npm test",
    "mocha:json": "mocha --reporter json > coverage/mocha.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octoblu/zooid-ui-bluprint-config-builder.git"
  },
  "keywords": [
    "zooid",
    "zooid-ui",
    "react",
    "reactjs",
    "octoblu"
  ],
  "author": "Octoblu Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octoblu/zooid-ui-bluprint-config-builder/issues"
  },
  "homepage": "https://github.com/octoblu/zooid-ui-bluprint-config-builder#readme",
  "nyc": {
    "include": [
      "src"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ],
    "cache": true,
    "reporter": [
      "text-summary",
      "lcov",
      "json"
    ],
    "require": [
      "babel-core/register",
      "babel-polyfill"
    ],
    "extension": [
      ".coffee"
    ]
  },
  "devDependencies": {
    "@kadira/storybook": "^1.36.0",
    "amdefine": "^1.0.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.3.2",
    "eslint-plugin-import": "^1.10.0",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "ignore-styles": "^4.0.0",
    "jsdom": "^9.3.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "mocha-multi": "^0.9.0",
    "mocha-osx-reporter": "^0.1.2",
    "null-loader": "^0.1.1",
    "nyc": "^6.6.1",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "lodash": "^4.13.1",
    "react": "^15.0.1",
    "react-jsonschema-form": "^0.34.1",
    "zooid-form-field": "^1.0.3",
    "zooid-form-label": "^1.0.1",
    "zooid-heading": "^1.0.5",
    "zooid-input": "^2.1.1",
    "zooid-list": "^1.2.0",
    "zooid-switch": "^2.0.0",
    "zooid-ui": "^6.0.0"
  }
}
